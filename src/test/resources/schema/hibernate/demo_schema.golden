
    create table domain (
       eppRepoId varchar(255) not null,
        fullyQualifiedDomainName varchar(255),
        tld varchar(255),
        primary key (eppRepoId)
    );

    create table host (
       eppRepoId varchar(255) not null,
        fullyQualifiedHostName varchar(255),
        primary key (eppRepoId)
    );
